// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id         String @id @default(uuid())
  name       String 
  email      String @unique
  password   String
  avatar_url String @default("")
}

model Schedule {
  id        String   @id @default(uuid())
  name_user String
  phone     String
  date      DateTime
}
